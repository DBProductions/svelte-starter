{
  "name": "svelte-starter",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start:cypress": "npx cypress open",
    "test:cypress": "npx cypress run --headless --config video=false -b chrome",
    "svelte-check": "svelte-check",
    "format": "npx prettier --write \"{,!(node_modules)/**/}*.{js,svelte}\"",
    "bundle": "npm run format && npm run svelte-check && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DBProductions/svelte-starter"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.0",
    "cypress": "^13.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "rollup": "^3.28.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-svelte": "^7.1.4",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1"
  }
}
