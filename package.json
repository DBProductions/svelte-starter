{
  "name": "svelte-starter",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "build": "npx rollup -c",
    "dev": "npx rollup -c -w",
    "test": "vitest run src",
    "test:watch": "vitest src",
    "test:coverage": "vitest run --coverage",
    "start:cypress": "npx cypress open",
    "test:cypress": "npx cypress run --headless --config video=false -b chrome",
    "svelte-check": "svelte-check",
    "format": "npx prettier --write \"{,!(node_modules)/**/}*.{js,svelte}\"",
    "bundle": "npm run format && npm run svelte-check && npm run test:coverage && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DBProductions/svelte-starter"
  },
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/svelte": "^5.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.13",
    "@vitest/coverage-v8": "^1.6.0",
    "cypress": "^13.10.0",
    "jest": "^29.7.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "rollup": "^4.18.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-svelte": "^7.2.0",
    "svelte": "^4.2.17",
    "svelte-check": "^3.8.0",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  }
}
